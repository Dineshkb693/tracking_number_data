{
  "name": "tracking_number_data",
  "version": "1.3.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkeen/tracking_number_data.git"
  },
  "author": "jkeen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkeen/tracking_number_data/issues"
  },
  "homepage": "https://github.com/jkeen/tracking_number_data#readme",
  "release": {
    "branches": ["main"],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "*!",
              "release": "major"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "tracking_number_data changelog",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": ["CHANGELOG.md"],
          "message": "${nextRelease.version} CHANGELOG [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "debug": false,
    "dryRun": false
  }
}
